<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kt.sangsang">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".SalesManagerActivity"/>
        <activity android:name=".SalesManagerSettingActivity" />
        <activity android:name=".SalesManagerMainActivity" />
        <activity android:name=".SalesManagerModifyActivity" />
        <activity android:name=".OrderManagerActivity" />
        <activity android:name=".OrderRegisterActivity" />
        <activity android:name=".OrderSalesActivity" />
        <activity android:name=".OrderAddActivity" />
    </application>

</manifest>